<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_list"
    android:id="@+id/l1"
    tools:context="com.example.sushant.parden.urb3">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq8"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="10dp"
            android:text="@string/uq8"
            android:textSize="15dp"
            />
        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/uq8"
            android:background="@drawable/shape"
            android:id="@+id/a8_d"
            android:hint="@string/a8d"
            android:inputType="number"
            android:textColorHint="@color/colorPrimary"
            android:textColor="@color/colorPrimary"/>
        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/uq8"
            android:layout_toRightOf="@+id/a8_d"
            android:layout_marginLeft="30dp"
            android:inputType="number"
            android:background="@drawable/shape"
            android:id="@+id/a8_m"
            android:hint="@string/a8m"
            android:textColorHint="@color/colorPrimary"
            android:textColor="@color/colorPrimary"/>

        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/uq8"
            android:layout_toRightOf="@+id/a8_m"
            android:layout_marginLeft="30dp"
            android:inputType="number"
            android:background="@drawable/shape"
            android:id="@+id/a8_y"
            android:hint="@string/a8y"
            android:textColorHint="@color/colorPrimary"
            android:textColor="@color/colorPrimary"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq9"
            android:layout_marginTop="170dp"
            android:layout_marginLeft="10dp"
            android:text="@string/uq9"
            android:textSize="15dp"
            />
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/uq9"
            android:orientation="horizontal"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/urg9"
            android:layout_marginBottom="5dp"
            >
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/yes"
                android:id="@+id/a5rb1"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:id="@+id/a5rb2"
                android:layout_marginLeft="170dp"
                android:text="@string/no"
                android:buttonTint="@color/colorPrimary"
                />
        </RadioGroup>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq10"
            android:layout_marginTop="290dp"
            android:text="@string/uq10"
            android:textSize="15dp"/>
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/uq10"
            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/urg10"
            android:layout_marginBottom="5dp"
            >
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a10rb1"
                android:id="@+id/a10rb1"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="@string/a10rb2"
                android:id="@+id/a10rb2"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a10rb3"
                android:id="@+id/a10rb3"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:id="@+id/a10rb4"
                android:layout_marginLeft="30dp"
                android:text="@string/a10rb4"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a10rb5"
                android:id="@+id/a10rb5"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
        </RadioGroup>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:id="@+id/b1"
            android:layout_marginBottom="100dp"
            android:layout_below="@id/urg10"
            android:text="@string/next"
            android:textColor="@color/colorPrimary"
            android:layout_marginTop="50dp"
            />
    </RelativeLayout>
    </ScrollView>
    </RelativeLayout>