<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_list"
    android:id="@+id/l1"
    tools:context="com.example.sushant.parden.urb7">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/uq20"
        android:layout_marginTop="40dp"
        android:text="@string/uq20"
        android:textSize="15dp"/>
    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/uq20"
        android:orientation="vertical"
        android:background="@drawable/shape"
        android:layout_marginTop="10dp"
        android:id="@+id/urg20"
        android:layout_marginBottom="5dp"
        >
        <RadioButton
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/a10rb1"
            android:id="@+id/a20rb1"
            android:layout_marginLeft="30dp"
            android:buttonTint="@color/colorPrimary"
            />
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginLeft="30dp"
            android:text="@string/a10rb2"
            android:id="@+id/a20rb2"
            android:buttonTint="@color/colorPrimary"
            />
        <RadioButton
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/a10rb3"
            android:id="@+id/a20rb3"
            android:layout_marginLeft="30dp"
            android:buttonTint="@color/colorPrimary"
            />
        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginLeft="30dp"
            android:text="@string/a10rb4"
            android:id="@+id/a20rb4"
            android:buttonTint="@color/colorPrimary"
            />
        <RadioButton
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/a10rb5"
            android:id="@+id/a20rb5"
            android:layout_marginLeft="30dp"
            android:buttonTint="@color/colorPrimary"
            />
    </RadioGroup>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq21"
            android:layout_marginTop="290dp"
            android:layout_marginLeft="10dp"
            android:text="@string/uq21"
            android:textSize="15dp"
            />
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/uq21"
            android:orientation="horizontal"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/urg21"
            android:layout_marginBottom="5dp"
            >
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/yes"
                android:id="@+id/a21rb1"
                android:layout_marginLeft="30dp"
                android:layout_below="@id/uq2"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/a21rb2"
                android:textColor="#000000"
                android:layout_toRightOf="@id/a21rb1"
                android:layout_marginLeft="170dp"
                android:text="@string/no"
                android:buttonTint="@color/colorPrimary"
                />
        </RadioGroup>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq22"
            android:layout_marginTop="410dp"
            android:layout_marginLeft="10dp"
            android:text="@string/uq22"
            android:textSize="15dp"
            />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_below="@+id/uq22"
            android:background="@drawable/shape"
            android:id="@+id/a22"
            android:textColor="@color/colorPrimary"
            tools:text="" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq23"
            android:layout_marginTop="520dp"
            android:layout_marginLeft="10dp"
            android:text="@string/uq23"
            android:textSize="15dp"
            />

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/uq23"
            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/urg23"
            android:layout_marginBottom="5dp" >
        <CheckBox
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/a23m1"
            android:id="@+id/a23rb1"
            android:layout_marginLeft="30dp"
            android:buttonTint="@color/colorPrimary"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginLeft="30dp"
            android:text="@string/a23m2"
            android:id="@+id/a23rb2"
            android:buttonTint="@color/colorPrimary"
            />
        <CheckBox
            android:textColor="#000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/a23m3"
            android:id="@+id/a23rb3"
            android:layout_marginLeft="30dp"
            android:buttonTint="@color/colorPrimary"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginLeft="30dp"
            android:text="@string/a23m4"
            android:id="@+id/a23rb4"
            android:buttonTint="@color/colorPrimary"
            />
        </RadioGroup>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:id="@+id/b1"
            android:layout_marginBottom="100dp"
            android:layout_below="@id/urg23"
            android:text="@string/next"
            android:textColor="@color/colorPrimary"
            android:layout_marginTop="50dp"
            />
    </RelativeLayout>
    </ScrollView>
</RelativeLayout>