<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_list"
    android:id="@+id/l1"
    tools:context="com.example.sushant.parden.urb12">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq34"
            android:layout_marginTop="40dp"
            android:text="@string/uq34"
            android:textSize="15dp"/>.
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/uq34"
            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/urg34"
            android:layout_marginBottom="5dp"
            >
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a24rb1"
                android:id="@+id/a34rb1"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="@string/a24rb2"
                android:id="@+id/a34rb2"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a24rb3"
                android:id="@+id/a34rb3"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="@string/a24rb4"
                android:id="@+id/a34rb4"
                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a24rb5"
                android:id="@+id/a34rb5"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
        </RadioGroup>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/uq35"
            android:layout_marginTop="320dp"
            android:text="@string/uq35"
            android:textSize="15dp"/>.
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/uq35"
            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/urg35"
            android:layout_marginBottom="5dp"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >
                <TextView
                    android:textColor="#000000"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/square"
                    android:text=" "
                    android:textAlignment="center"
                    android:id="@+id/box1"
                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"
                    />

                <TextView
                android:textColor="#000000"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginTop="5dp" android:textAlignment="center"
                android:background="@drawable/square"
                android:text=" "
                android:id="@+id/box2"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
                <TextView
                    android:textColor="#000000"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:text=" "
                    android:textAlignment="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/square"
                    android:id="@+id/box3"
                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"
                    />
                <TextView
                    android:textColor="#000000"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:text=" "
                    android:textAlignment="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/square"
                    android:id="@+id/box4"
                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"
                    />
                <TextView
                    android:textColor="#000000"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:text=" "
                    android:layout_marginTop="5dp"
                    android:background="@drawable/square"
                    android:id="@+id/box5"
                    android:textAlignment="center"
                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"
                    />
                <TextView
                    android:id="@+id/box6"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="20dp"
                    android:textAlignment="center"
                    android:background="@drawable/square"
                    android:buttonTint="@color/colorPrimary"
                    android:text=" "
                    android:textColor="#000000" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical">
            <TextView
                android:textColor="#000000"
                android:textAlignment="center"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a35rb1"
                android:id="@+id/a35rb1"
                android:buttonTint="@color/colorPrimary"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="30dp"
                android:text="@string/a35rb2"
                android:id="@+id/a35rb2"
                android:buttonTint="@color/colorPrimary"
                />
            <TextView
                android:textColor="#000000"
                android:layout_marginTop="12dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a35rb3"
                android:id="@+id/a35rb3"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="30dp"
                android:text="@string/a35rb4"
                android:id="@+id/a35rb4"
                android:buttonTint="@color/colorPrimary"
                />
            <TextView
                android:textColor="#000000"
                android:layout_marginTop="12dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/a35rb5"
                android:id="@+id/a35rb5"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            <TextView
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="@string/a35rb6"
                android:id="@+id/a35rb6"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                />
            </LinearLayout>
            </LinearLayout>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reset"
                android:id="@+id/reset"
                android:layout_marginLeft="130dp"
                />
        </RadioGroup>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:id="@+id/b1"
            android:layout_marginBottom="100dp"
            android:layout_below="@id/urg35"
            android:text="@string/next"
            android:textColor="@color/colorPrimary"
            android:layout_marginTop="50dp"
            />
    </RelativeLayout>
    </ScrollView>
</RelativeLayout>