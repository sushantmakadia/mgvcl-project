                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:inputType="date"
                android:layout_marginTop="820dp"
                android:text="@string/q8"
                android:textSize="15dp" />
            <EditText
