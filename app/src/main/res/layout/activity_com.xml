                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"

                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:id="@+id/rb10_8"
                    android:layout_toRightOf="@id/rb4"
                    android:layout_marginLeft="30dp"
                    android:text="શોપિંગ મોલ"
                    android:buttonTint="@color/colorPrimary"

                    />
                <RadioButton
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="હોસ્પિટલ/રિસોર્ટ"
                    android:id="@+id/rb10_9"
                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"

                    />
                <RadioButton
                    android:textColor="#000000"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="હોટેલ/રિસોર્ટ"
                    android:id="@+id/rb10_10"
                    android:layout_marginLeft="30dp"
                    android:buttonTint="@color/colorPrimary"

                    />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    android:id="@+id/rb10_11"
                    android:layout_marginLeft="30dp"
                    android:text="ખાનગી/ શૈક્ષણીક સંસ્થા"

                    android:buttonTint="@color/colorPrimary"
