<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="horizontal"
    android:background="@drawable/gradient_list"
    android:id="@+id/l1"
    android:layout_height="match_parent"
    tools:context=".page11">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="28. શુ આપ સહમત છો કે કંપની ના સ્ટાફ વિવેકી, ગ્રાહકલક્ષી અને કાર્યક્ષમ છે? "
            android:id="@+id/r29tv"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="10dp"

            />
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/r29rg"
            android:layout_marginBottom="5dp"

            >

            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ભાર પૂર્વક અસહમત! "
                android:id="@+id/r29rb1"
                android:layout_marginLeft="30dp"

                android:buttonTint="@color/colorPrimary"

                />

            <RadioButton
                android:id="@+id/r29rb2"
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                android:text="અસહમત"
                android:textColor="#000000"

                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="સહમત"
                android:id="@+id/r29rb3"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"

                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="ભાર પૂર્વક સહમત "
                android:id="@+id/r129rb4"

                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="ભાર પૂર્વક સહમત "
                android:id="@+id/r29rb5"

                android:buttonTint="@color/colorPrimary"
                />
        </RadioGroup>

        <TextView
            android:id="@+id/r30tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="30dp"
            android:text="29. કંપની દ્વારા  પૂરી પાડવામાં આવતી વિદ્યુત શક્તિ અને સેવાઓ ને ધ્યાન માં લેતા આપ કંપની દ્વારા  લેવામાં આવતી કીમત વિષે શું માનો છો?"
            android:textSize="15dp" />

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/r30rg"
            android:layout_marginBottom="5dp"

            >

            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="મોંઘી "
                android:id="@+id/r30rb1"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"

                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="વ્યાજબી  "
                android:buttonTint="@color/colorPrimary"
                android:id="@+id/r30rb2"

                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" સસ્તી  "
                android:id="@+id/r30rb3"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"

                />



        </RadioGroup>

        <TextView
            android:id="@+id/r31tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="30dp"
            android:text="30. શું તમને એવુ લાગે છે કે છેલ્લા એક વર્ષમાં ,વીજ પુરવઠો અને તેને લગતી સેવાઓ માં સુધારો આવ્યો છે ખરો? "

            />
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp"
            android:id="@+id/r31rg"
            android:layout_marginBottom="5dp"

            >

            <RadioButton
                android:id="@+id/r31rb1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"
                android:text="અતિશય બગડી છે  "

                android:textColor="#000000"

                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="કૈક અંશે બગડી છે "
                android:buttonTint="@color/colorPrimary"
                android:id="@+id/r31rb2"

                />
            <RadioButton
                android:textColor="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ન તો બગડી છે , ન તો સુધરી છે "
                android:id="@+id/r31rb3"
                android:layout_marginLeft="30dp"
                android:buttonTint="@color/colorPrimary"

                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="વ્યાજબી સુધારો છે "
                android:id="@+id/r131rb4"

                android:buttonTint="@color/colorPrimary"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:text="અત્યત સુધારો છે"
                android:id="@+id/r31rb5"

                android:buttonTint="@color/colorPrimary"
                />
        </RadioGroup>

        <Button
            android:id="@+id/rB22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:text="@string/next"
            android:textColor="#151719"
            android:layout_marginBottom="100dp"
            android:layout_marginTop="50dp"
            />
    </LinearLayout>
    </ScrollView>
</LinearLayout>